{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "object-literal-sort-keys":false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "quotemark": [true, "single", "double", "backtick", "avoid-escape"]
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}